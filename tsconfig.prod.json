{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "declaration": true,
    "noEmit": true,
    "module": "ES2020",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true
  }
}
